# Custom Widgets -----------------------------------------------
<AppoinBox>:
    orientation: "vertical"
    spacing: "5dp"
    size_hint_y: None
    height: "200dp"
    MDBoxLayout:
        spacing: "10dp"
        MDRectangleFlatIconButton:
            icon:'calendar'
            text:'Select date'
            on_release: 
                root.calender()
        MDRectangleFlatIconButton:
            icon:'clock'
            text:'Select time'
            on_release: 
                root.time()
        MDIconButton:
            icon:'refresh-circle'
            on_release: root.reload()
    MDBoxLayout:
        spacing: "10dp"
        MDLabel:
            id:date_choosed
            text:'date choose'
            font_style:'Button'
        MDLabel:
            id:time_choosed
            text:'time choose'
            font_style:'Button'
    MDLabel:
        text:'KINDLY NOTE:'
        font_style:'Caption'
        italic:True
    MDLabel:
        text:'Once the appoinment is booked it will be sent for confirmation. Payment can be done once the booking is confirmed.'
        font_style:'Caption'
    MDRectangleFlatIconButton:
        icon:'content-save-all'
        text:'Save & Book'
        on_press:root.book_appoin()
<CommonChip@MDChip>
    # Custom attribute.
    text: ""
    icon: ""
    # Chip attribute.
    type: "assist"
    md_bg_color: "#8C665E"
    line_color: "grey"
    elevation: 1
    shadow_softness: 2

    MDChipLeadingIcon:
        icon: root.icon
        theme_text_color: "Custom"
        text_color: "#A0D991"

    MDChipText:
        text: root.text
        theme_text_color: "Custom"
        text_color: "#A0D991"
# A Card used for Doctor screen to display all appoin.
<AppoinCheckCard>:
    pname:''
    app_datetime2:''
    statuss:''
    emaill:''
    orientation: "vertical"
    padding: "2dp"
    size_hint: None, None
    size: "340dp", "110dp"  
    #height: "160dp"
    pos_hint: {"center_x": .5, "center_y": .5}
    MDBoxLayout:
        size_hint_y: .8
        padding: [20,0,0,0]
        spacing: '10dp'
        #md_bg_color: app.theme_cls.primary_color
        MDIcon:
            icon:'account'
            size_hint:None,None
            size:45,45
            pos_hint: {"center_x": .1, "center_y": .2}
        MDLabel:
            id:pname
            text: root.pname
            #theme_text_color: 'Custom'
            font_style:'H6'
            #text_color: 1, 1, 1, 1
        MDIconButton:
            theme_text_color: 'Custom' 
            icon: 'card-account-details'
            disabled: True if root.ids.statuss.text=='appoinment---done' else False
            pos_hint: {"center_x": .85, "center_y": .4} 
            on_release: root.show_pdtls(root.ids.emaill.text)
        MDIconButton: 
            theme_text_color: 'Custom' 
            icon: 'check'
            disabled: False if root.ids.statuss.text=='processing' else True
            pos_hint: {"center_x": .85, "center_y": .4} 
            on_release: root.confirm_appoin(root.ids.emaill.text,root.ids.app_datetime2.text,root.ids.statuss.text)
        MDIconButton:
            icon:'cancel'
            disabled: False if root.ids.statuss.text=='processing' else True
            pos_hint: {"center_x": .65, "center_y": .4}
            on_release: root.reject_appoin(root.ids.emaill.text,root.ids.app_datetime2.text,root.ids.statuss.text)
    MDLabel:
        id:app_datetime2
        font_style:'Body1'
        text: root.app_datetime2
        size_hint_y:None
        color:0,0,0,1
        pos_hint: {"center_x": .4, "center_y": .5}
        #height:self.texture_size[1]
        halign:'center'
        theme_text_color: 'Custom'
        text_color: 1, 1, 1, 1
    MDBoxLayout:
        size_hint_y: .5
        padding: [20,0,0,0]
        spacing: '10dp'
        MDLabel:
            id:statuss
            text:root.statuss
            font_style:'Body2'
            color:1,1,1,1
        MDLabel:
            id:emaill
            text:root.emaill
            font_style:'Body2'
            color:0,0,0,0
        MDRectangleFlatIconButton:
            icon:'account-multiple-check'
            text:'Done'
            #disabled: False if root.ids.statuss.text=='confirmed-booking' else True
            on_release: root.appoin_done(root.ids.emaill.text,root.ids.app_datetime2.text,root.ids.statuss.text)
